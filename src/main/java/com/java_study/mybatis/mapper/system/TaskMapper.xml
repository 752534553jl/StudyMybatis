<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.java_study.mybatis.mapper.system.TaskMapper">
    <select id="getTask" parameterType="long" resultType="com.java_study.mybatis.model.system.Task">
        select id,title,context,note from t_task where id=#{id}
    </select>
</mapper>